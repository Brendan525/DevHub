@model List<Question>
@{
    ViewData["Title"] = "Index";
}

<div>
    <form method="get" action="/DevHub/">
        <input type="text" name="search" />
        <input type="submit" value="Search" />
    </form>
</div>

<h3>@ViewData["Message"]</h3>
<div>    
    @foreach (Question q in Model)
    {
        <div>
            <a class="btn btn-dark" style="margin: 15px" asp-action="Detail" asp-controller="DevHub" asp-route-id="@q.id">@q.title (@q.username)</a>
        </div>
    }
</div>
